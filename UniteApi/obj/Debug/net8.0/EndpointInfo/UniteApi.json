  "components": {
    "schemas": {
      "QuaternionData": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number",
            "format": "float"
          },
          "y": {
            "type": "number",
            "format": "float"
          },
          "z": {
            "type": "number",
            "format": "float"
          },
          "w": {
            "type": "number",
            "format": "float"
          }
        },
        "additionalProperties": false
      },
      "User": {
        "required": [
          "passwordHash",
          "username"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "passwordHash": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Vector3Data": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number",
            "format": "float"
          },
          "y": {
            "type": "number",
            "format": "float"
          },
          "z": {
            "type": "number",
            "format": "float"
          }
        },
        "additionalProperties": false
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "summary": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "WorldData": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "slot": {
            "type": "integer",
            "format": "int32"
          },
          "worldObjects": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/WorldObjectData"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "WorldEntity": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "slot": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "worldObjects": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/WorldObjectEntity"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "WorldObjectData": {
        "type": "object",
        "properties": {
          "prefabName": {
            "type": "string",
            "nullable": true
          },
          "position": {
            "$ref": "#/components/schemas/Vector3Data"
          },
          "scale": {
            "$ref": "#/components/schemas/Vector3Data"
          },
          "rotation": {
            "$ref": "#/components/schemas/QuaternionData"
          }
        },
        "additionalProperties": false
      },
      "WorldObjectEntity": {
        "required": [
          "position",
          "prefabName",
          "rotation",
          "scale"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "prefabName": {
            "minLength": 1,
            "type": "string"
          },
          "position": {
            "$ref": "#/components/schemas/Vector3Data"
          },
          "scale": {
            "$ref": "#/components/schemas/Vector3Data"
          },
          "rotation": {
            "$ref": "#/components/schemas/QuaternionData"
          },
          "worldEntityId": {
            "type": "integer",
            "format": "int32"
          },
          "world": {
            "$ref": "#/components/schemas/WorldEntity"
          }
        },
        "additionalProperties": false
      }
    }
  }
}